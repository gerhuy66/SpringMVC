<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace = "SystemMapper">
	<select id = "selectUser" parameterType="java.lang.String" resultType="com.model.User">
		SELECT id,name
		FROM User
		WHERE id = #{id}
	</select>
	
	<select id = "selectContact" parameterType="java.lang.String" resultType="com.model.Contact">
		SELECT *
		FROM contact
		WHERE c_id = #{id}
	</select> 
</mapper>